            appboy.initialize(BRAZE_API_KEYS['real'], {
                baseUrl: BRAZE_SDK_KEYS,
                allowUserSuppliedJavascript: true,
                enableLogging: 'real' === 'real' ? false : true,
            });

            if (userId) {
