      if (!sessionStorage.getItem('preserveTranslationInfo')) {
        // 다른탭으로 넘어갈 경우 한글로 초기화 하도록 wglang 제거
        if (localStorage) { // localStorage 가 앱에서 못찾는 경우가 있어서 방어 로직 추가
          localStorage.removeItem('wglang');
        }
      } else {
